
public class Main {

	public static void main(String[] args) {
		System.out.println("Divide by zero program ");
		int num = 50;
		int result;
		try {
			result=50/0;
	
		System.out.println("result is: "+result);
		}
		catch(ArithmeticException ex)
		{
		System.out.println(ex);
	}
		System.out.println("Rest of the code");
	}
}
